# ERROR in Line 19 Col 11
# 
#     while not stop(i)
#           ^
# Invalid conditional expression

fn stop(i: int) -> bool
{
    if(i != 5) return true

    return false
}

fn main() -> int
{
    var i: int = 0

    while not stop(i)
    {
        println(i)
        i += 1
    }
}
# 3

fn BSearch(a:int[10], S:int, L:int, x:int) -> int
{
    var H:int = L/2
    
    if(L == 1)
        if(a[S] == x) return S
        else return -1
    else if(x < a[S+H])
        return BSearch(a, S, H, x)
    else
        return BSearch(a, S+H, L-H, x)
}

fn main()
{
    var a:int[10] = [34, 5, 6, 23, 67, 45, 67, 35, 34, 51]
    println(BSearch(a, 0, 10, 23)) # 3
}

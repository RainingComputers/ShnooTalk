generic P, Q, R, S

struct Quad
{
    var first: P
    var second: Q
    var third: R
    var fourth: S
}

fn quad(first: P, second: Q, third: R, fourth: S) -> Quad
{
    var self: Quad
    self.first = first
    self.second = second
    self.third = third
    self.fourth = fourth
    return self
}

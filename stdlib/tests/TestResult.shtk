from "../stdlib/Result.shtk" use Result, ok, fail

enum TestErrorEnum {TEST_ERROR_0, TEST_ERROR_1}

fn main() -> int
{
    const a := ok[int, TestErrorEnum](10)
    println(a.hasValue())
    println(a.isError())
    println(a.expect())
    println(a | 2)

    const [itemA, errA] := a
    println(itemA)

    const b := fail[int, TestErrorEnum](TEST_ERROR_1)
    println(b.hasValue())
    println(b.isError())
    println(b | 100)

    const [itemB, errB] := b
    println(errB.getError() == TEST_ERROR_1)

    const c := a | b
    println(c.hasValue())
    println(c.expect())

    const d := fail[int, TestErrorEnum](TEST_ERROR_0)
    const e := d | b
    const [itemE, errE] := e
    println(errE.getError() == TEST_ERROR_0)

    return 0
}
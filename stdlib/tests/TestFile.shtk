use "../stdlib/File.shtk" as file

fn main() -> int
{
    const fileOptional1 := file::open("TestSliceCopyCapacityPanic.shtk", "r")
    const file1 := fileOptional1.expect()

    println(file1.readLine().expect().toCharArray())

    file1.close()

    const fileOptional2 := file::open("TestSliceCopyCapacityPanic.shtk", "r")
    const file2 := fileOptional2.expect()

    println(file2.read().toCharArray())

    file2.seek(-10, file::SEEK_END)

    println("tell", file2.tell())

    println(file2.read().toCharArray())

    file2.close()

    return 0
}